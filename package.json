{
  "name": "eforged",
  "productName": "eForged",
  "version": "0.0.1",
  "description": "An Electron and Angular App",
  "repository": "https://github.com/sourcygen/electron-angular-quick-start.git",
  "author": "lionfree",
  "license": "MIT",
  "main": ".webpack/main",
  "scripts": {
    "start": "npm-run-all -p start:common start:renderer start:main-process",
    "start:common": "tsc -b workspaces/common -w",
    "start:renderer": "wait-on workspaces/common/.dist/index.d.ts && cd workspaces/renderer && npm run start",
    "start:main-process": "node scripts/electron-forge-start-watch.js",
    "start:main-process:once": "wait-on http://localhost:4200 && electron-forge start",
    "package": "npm run package:renderer && npm run package:main-process",
    "package:common": "tsc -b workspaces/common",
    "package:renderer": "npm run package:common && cd workspaces/renderer && npm run package",
    "package:main-process": "npm run package:common && electron-forge package",
    "make": "npm run package:renderer && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "npm run package:common && eslint --ext .ts .",
    "test:e2e": "npm run test:angular-e2e && npm run test:electron-e2e",
    "test:angular-e2e": "npm-run-all -p -r start start:angular-e2e",
    "start:angular-e2e": "wait-on http://localhost:4200 && cd workspaces/renderer && npm run cypress:run",
    "test:electron-e2e": "npm run package && npm run test:electron-e2e:wdio-only",
    "test:electron-e2e:wdio-only": "cross-env X_NODE_ENV=e2e-test wdio run workspaces/electron-e2e/webdriverio.config.ts --autoCompileOpts.tsNodeOpts.project=workspaces/electron-e2e/tsconfig.json",
    "clean": "shx rm -rf .webpack out allure-results package-lock.json node_modules workspaces/common/.dist workspaces/renderer/node_modules workspaces/renderer/.angular workspaces/renderer/.dist",
    "outdated-deps": "npm run outdated-deps:main-process && npm run outdated-deps:renderer && npm run outdated-deps:common",
    "outdated-deps:main-process": "npx ncu",
    "outdated-deps:renderer": "cd workspaces/renderer && npx ncu",
    "outdated-deps:common": "cd workspaces/common && npx ncu",
    "update-deps": "npm run update-deps:main-process && npm run update-deps:renderer && npm run update-deps:common",
    "update-deps:main-process": "npx ncu -u",
    "update-deps:renderer": "cd workspaces/renderer && ng update @angular/cli @angular/core --force && npx ncu -u && cd ../.. && npm run clean",
    "update-deps:common": "cd workspaces/common && npx ncu -u",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-dmg": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "@electron-forge/plugin-webpack": "6.0.5",
    "@types/jasmine": "^4.3.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "@wdio/allure-reporter": "^8.5.0",
    "@wdio/cli": "^8.5.5",
    "@wdio/jasmine-framework": "^8.5.5",
    "@wdio/local-runner": "^8.5.5",
    "@wdio/spec-reporter": "^8.4.0",
    "allure-commandline": "^2.21.0",
    "chokidar-cli": "^3.0.0",
    "chromedriver": "^110.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^23.1.2",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "expect-webdriverio": "^4.1.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "jasmine": "^4.5.0",
    "lint-staged": "^13.1.2",
    "node-loader": "^2.0.0",
    "npm-check-updates": "^16.7.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "release-please": "^15.8.0",
    "shx": "^0.3.4",
    "style-loader": "^3.3.1",
    "tree-kill": "^1.2.2",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "wdio-chromedriver-service": "^8.1.1",
    "wdio-electron-service": "^4.0.0",
    "wdio-wait-for": "^3.0.2"
  },
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^11.1.0",
    "winston": "^3.8.2"
  },
  "lint-staged": {
    "*.ts": "npm run lint"
  },
  "workspaces": [
    "workspaces/renderer",
    "workspaces/common"
  ],
  "keywords": [
    "angular",
    "angular 14",
    "electron",
    "electron 18",
    "typescript",
    "typescript 4",
    "wdio",
    "webdriverio",
    "chromedriver",
    "scss",
    "live reload"
  ]
}
